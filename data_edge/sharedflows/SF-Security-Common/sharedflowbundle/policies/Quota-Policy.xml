<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Quota continueOnError="false" enabled="true" name="Quota-Policy">
    <DisplayName>Quota Policy</DisplayName>
    <Properties/>
    <Allow count="1000" countRef="request.header.allowed-quota"/>
    <Interval>1</Interval>
    <TimeUnit>hour</TimeUnit>
    <Identifier ref="request.header.client-id"/>
</Quota>